class LinkedList {

	Node head;
	static class Node {
		int data;
		Node next;

		Node(int d) {
			data = d;
			next = null;
		}
	}

	public static void main(String[] args) {
		LinkedList l = new LinkedList();

		l.insert(4);
		l.insert(3);
		l.insert(2);
		l.insert(1); 

		l.nthFromLast(2);
		l.printList();
	}

	public void insert(int data) {
		Node add = new Node(data);
		add.next = head;
		head = add;
	}

	public void printList() {
		Node n = head;
		while (n != null) {
			System.out.print(n.data + " ");
			n = n.next;
		}
	}

	public void nthFromLast(int position) {
		Node n = head;
		Node m = head;
		int count = 0;
		while (count != position) {
			count++;
			n = n.next;
		}
		while (n != null) {
			n = n.next;
			m = m.next;
		}
		System.out.println(m.data);
	}
}
